<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="shopPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>model.Chitietdonhang</class>
    <class>model.Donhang</class>
    <class>model.Quantri</class>
    <class>model.Quangcao</class>
    <class>model.Khachhang</class>
    <class>model.Cauhinh</class>
    <class>model.Nhomsanpham</class>
    <class>model.Sequence</class>
    <class>model.Nhacungcap</class>
    <class>model.Uathich</class>
    <class>model.Vanchuyen</class>
    <class>model.Thanhtoan</class>
    <class>model.History</class>
    <class>model.Sanpham</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/shopping?zeroDateTimeBehavior=CONVERT_TO_NULL"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="phuc"/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
